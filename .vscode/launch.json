{
  "version": "0.2.0",
  "configurations": [
   
    {
      "name": "Debug print",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/examples/print",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/examples",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
   
  ]
}
